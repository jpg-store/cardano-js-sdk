{
  "name": "@cardano-sdk/input-selection",
  "version": "0.13.6",
  "description": "TypeScript definitions for input-selection (Coin Selection Algorithms for Cardano)",
  "engines": {
    "node": ">=16.20.2"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": "https://github.com/input-output-hk/cardano-js-sdk",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc -p ./tsconfig.esm.json &&  cp ../../build/esm-package.json ./dist/esm/package.json",
    "build:cjs": "tsc -p src/tsconfig.json && cp ../../build/cjs-package.json ./dist/cjs/package.json",
    "circular-deps:check": "madge --circular dist/cjs",
    "tscNoEmit": "echo typescript --noEmit command not implemented yet",
    "cleanup:dist": "rm -rf dist",
    "cleanup:nm": "rm -rf node_modules",
    "cleanup": "rm -rf dist node_modules",
    "lint": "eslint -c ../../complete.eslintrc.js \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest -c ./jest.config.js",
    "test:build:verify": "tsc --build ./test",
    "test:e2e": "echo 'test:e2e' command not implemented yet",
    "coverage": "yarn test --coverage || true",
    "prepack": "yarn build",
    "test:debug": "DEBUG=true yarn test"
  },
  "devDependencies": {
    "@cardano-sdk/util-dev": "workspace:~",
    "@types/lodash": "^4.14.182",
    "fast-check": "^2.17.0",
    "lodash": "^4.17.21",
    "madge": "^5.0.1"
  },
  "dependencies": {
    "@cardano-sdk/core": "workspace:~",
    "@cardano-sdk/key-management": "workspace:~",
    "@cardano-sdk/util": "workspace:~",
    "bignumber.js": "^9.1.1",
    "lodash": "^4.17.21",
    "ts-custom-error": "^3.2.0"
  },
  "files": [
    "dist/*",
    "!dist/tsconfig.tsbuildinfo",
    "LICENSE",
    "NOTICE"
  ]
}
